{
  "hosting": {
    "public": "_site",
    "ignore": [
      "firebase.json",
      "static/css/**",
      "static/js/**"
    ],
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=60"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|png|webp|js|css|ico)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      },
      {
        "source": "**/*",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/contacts/",
        "destination": "/departments/",
        "type": 301
      },
      {
        "source": "/covid-financial-info-individuals/",
        "destination": "/covid-information/financial-info-individuals/",
        "type": 301
      },
      {
        "source": "/covid-business-information/",
        "destination": "/covid-information/business-information/",
        "type": 301
      },
      {
        "source": "/covid-archives/",
        "destination": "/covid-information/archives/",
        "type": 301
      },
      {
        "source": "/static/images/boroughfacebookicon.jpg",
        "destination": "/static/images/boroughfacebookicon_gray.jpg",
        "type": 301
      }
    ]
  }
}
